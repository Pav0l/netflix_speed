{

  "manifest_version": 2,
  "name": "Netflix_speed",
  "version": "1.0",

  "description": "Adds a button to change the speed of a video beyin played in the browser.",
  "homepage_url": "",
  "icons": {
    "64": "icons/icon-64.png"
  },
    "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/icon-64.png",
    "default_title": "Netflix_speed",
    "default_popup": "popup/index.html",
    "default_area": "navbar"
  },

  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*", "*://*.hbogo.sk/*"],
      "js": ["app.js"]
    }
  ]
}